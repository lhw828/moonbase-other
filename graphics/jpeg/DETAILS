          MODULE=jpeg
         VERSION=9c
          SOURCE=${MODULE}src.v$VERSION.tar.gz
      SOURCE_URL=http://www.ijg.org/files
      SOURCE_VFY=sha256:650250979303a649e21f87b5ccd02672af1ea6954b911342ea491f351ceb7122
