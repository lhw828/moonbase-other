 SOURCE_URL_FULL=https://github.com/twogood/unshield/archive/$VERSION.tar.gz
      SOURCE_VFY=sha256:aa8c978dc0eb1158d266eaddcd1852d6d71620ddfc82807fe4bf2e19022b7bab
