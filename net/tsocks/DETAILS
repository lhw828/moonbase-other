          MODULE=tsocks
         VERSION=1.8beta5
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=$SFORGE_URL/$MODULE/$MODULE/1.8%20beta%205
      SOURCE_VFY=sha256:849d7ef5af80d03e76cc05ed9fb8fa2bcc2b724b51ebfd1b6be11c7863f5b347
SOURCE_DIRECTORY=$BUILD_DIRECTORY/$MODULE-${VERSION:0:3}
        WEB_SITE=http://tsocks.sourceforge.net/
         ENTERED=20170808
         UPDATED=20170808
           SHORT="tunnel connections via SOCKS proxy"

cat << EOF
tsocks provides transparent network access through
a SOCKS version 4 or 5 proxy (usually on a firewall).
tsocks intercepts the calls applications make
to establish TCP connections and transparently
proxies them as necessary.
EOF
