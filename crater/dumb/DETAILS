          MODULE=dumb
         VERSION=0.9.3
          SOURCE=$MODULE-$VERSION.tar.gz
         SOURCE2=$MODULE-$VERSION-autotools.tar.gz
      SOURCE_URL=$SFORGE_URL/dumb/
     SOURCE2_URL=$SFORGE_URL/dumb/
