          MODULE=IO-Digest
         VERSION=0.11
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=http://search.cpan.org/CPAN/authors/id/C/CL/CLKAO/
      SOURCE_VFY=sha256:8ffcf85a7f6213e5e94140adcc2b179ed02498eadc84309457e904dec93f7f92
