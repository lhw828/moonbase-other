mquery ENABLE_SIX_AXIS "Enable the six axis plugin? " y "--enable-sixaxis" "--disable-sixaxis"
mquery ENABLE_EXPER    "Enable experimental plugins?" n "--enable-experimental" "--disable-experimental"
