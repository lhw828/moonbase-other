          MODULE=normalize
         VERSION=0.7.7
          SOURCE=$MODULE-$VERSION.tar.bz2
      SOURCE_URL=http://savannah.nongnu.org/download/$MODULE/
      SOURCE_VFY=sha256:ef9d8558515cc942518981d3db8fa2490fba6d6b5b74e0a3b75336fbc66c6bd8
