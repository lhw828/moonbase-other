          MODULE=p7zip
         VERSION=16.02
          SOURCE=${MODULE}_${VERSION}_src_all.tar.bz2
         SOURCE2=$MODULE-CVE-2016-9296.patch
         SOURCE3=$MODULE-CVE-2017-17969.patch
         SOURCE4=$MODULE-CVE-2018-5996.patch
SOURCE_DIRECTORY=$BUILD_DIRECTORY/${MODULE}_${VERSION}
      SOURCE_URL=https://ftp.osuosl.org/pub/blfs/conglomeration/$MODULE/
     SOURCE2_URL=$PATCH_URL
     SOURCE3_URL=$PATCH_URL
     SOURCE4_URL=$PATCH_URL
      SOURCE_VFY=sha256:5eb20ac0e2944f6cb9c2d51dd6c4518941c185347d4089ea89087ffdd6e2341f
     SOURCE2_VFY=sha256:f9bcbf21d4aa8938861a6cba992df13dec19538286e9ed747ccec6d9a4e8f983
     SOURCE3_VFY=sha256:b01ca4a334c0eabd36ef0ecb25d0ecdac170d8ea5c96da58ef88173691749265
